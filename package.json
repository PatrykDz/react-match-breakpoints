{
  "name": "react-match-breakpoints",
  "version": "0.1.0",
  "description": "Connected breakpoints for react application",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "start": "npm run start:dev && npm run start:docs",
    "start:dev": "webpack --progress --colors --watch --env dev",
    "start:docs": "npm --prefix ./docs install && npm --prefix ./docs start"
  },
  "author": "Michal Klim",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "watch": "^1.0.2",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  }
}
